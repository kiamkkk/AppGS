<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gseek.gs.dao.UserIdentificationMapper">

    <resultMap id="BaseResultMap" type="com.gseek.gs.pojo.UserIdentification">
        <id column="user_id" jdbcType="INTEGER" property="userId" />
        <result column="id_number" jdbcType="VARCHAR" property="idNumber" />
        <result column="certified" jdbcType="BIT" property="certified" />
        <result column="modified_time" jdbcType="BIGINT" property="modifiedTime" />
    </resultMap>

    <insert id="insertUserIdentification" parameterType="com.gseek.gs.pojo.UserIdentification">
        insert into user_identification (user_id, modified_time)
            values (#{userIdentification.userId},#{userIdentification.modifiedTime});
    </insert>

</mapper>